<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="frontRecruit" extends="front-default" namespace="/">
		<action name="queryRecruitInit" class="frontRecruitAction"
			method="queryRecruitInit">
			<result name="success">
				/WEB-INF/page/front/recruit_list_index.jsp
			</result>
		</action>
		<action name="queryRecruit" class="frontRecruitAction"
			method="queryRecruit">
			<result name="success">
				/WEB-INF/page/front/recruit_list.jsp
			</result>
		</action>
		<action name="queryRecruit2" class="frontRecruitAction"
			method="queryRecruit2">
			<result name="success">
				/WEB-INF/page/front/recruit_news.jsp
			</result>
		</action>
	</package>
</struts>