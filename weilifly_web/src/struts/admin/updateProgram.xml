<struts>
	<package name="updateProgram" extends="admin-default" namespace="/admin">
	
		<action name="queryUpdateProgramInit" class="updateProgramAction" method="queryUpdateProgramInit">
			<result name="success">/WEB-INF/page/admin/update_program_index.jsp</result>
		</action>
		
		<action name="queryUpdateProgramInfo" class="updateProgramAction" method="queryUpdateProgramInfo">
			<result name="success">/WEB-INF/page/admin/update_program_index_info.jsp</result>
		</action>
		
		<action name="addUpdateProgramInit" class="updateProgramAction" method="addUpdateProgramInit">
			<result name="success">/WEB-INF/page/admin/update_program_add.jsp</result>
		</action>
		
		<action name="addUpdateProgram" class="updateProgramAction" method="addUpdateProgram">
		</action>
		
		<action name="updateUpdateProgramInit" class="updateProgramAction" method="updateUpdateProgramInit">
			<result name="success">/WEB-INF/page/admin/update_program_update.jsp</result>
		</action>
		
		<action name="updateUpdateProgram" class="updateProgramAction" method="updateUpdateProgram">
		</action>
		
