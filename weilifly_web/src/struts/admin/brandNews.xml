<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="brandNews" extends="admin-default" namespace="/admin">
	
		<action name="queryBrandNewsInit" class="brandNewsAction" method="queryBrandNewsInit">
			<result name="success">/WEB-INF/page/admin/brand_news_index.jsp</result>
		</action>
		
		<action name="queryBrandNewsInfo" class="brandNewsAction" method="queryBrandNewsInfo">
			<result name="success">/WEB-INF/page/admin/brand_news_index_info.jsp</result>
		</action>
		
		<action name="addBrandNewsInit" class="brandNewsAction" method="addBrandNewsInit">
			<result name="success">/WEB-INF/page/admin/brand_news_add.jsp</result>
		</action>
		
		<action name="addBrandNews" class="brandNewsAction" method="addBrandNews">
		</action>
		
		<action name="updateBrandNewsInit" class="brandNewsAction" method="updateBrandNewsInit">
			<result name="success">/WEB-INF/page/admin/brand_news_update.jsp</result>
		</action>
		
		<action name="updateBrandNews" class="brandNewsAction" method="updateBrandNews">
		</action>
		
		<action name="deleteBrandNews" class="brandNewsAction" method="deleteBrandNews">
		</action>
		
	</package>
</struts>
