<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="newspaper" extends="admin-default" namespace="/admin">
	
		<action name="queryNewspaperInit" class="newspaperAction" method="queryNewspaperInit">
			<result name="success">/WEB-INF/page/admin/newspaper_index.jsp</result>
		</action>
		
		<action name="queryNewspaperInfo" class="newspaperAction" method="queryNewspaperInfo">
			<result name="success">/WEB-INF/page/admin/newspaper_index_info.jsp</result>
		</action>
		
		<action name="addNewspaperInit" class="newspaperAction" method="addNewspaperInit">
			<result name="success">/WEB-INF/page/admin/newspaper_add.jsp</result>
		</action>
		
		<action name="addNewspaper" class="newspaperAction" method="addNewspaper">
		</action>
		
		<action name="updateNewspaperInit" class="newspaperAction" method="updateNewspaperInit">
			<result name="success">/WEB-INF/page/admin/newspaper_update.jsp</result>
		</action>
		
		<action name="updateNewspaper" class="newspaperAction" method="updateNewspaper">
		</action>
		
		<action name="deleteNewspaper" class="newspaperAction" method="deleteNewspaper">
		</action>
		
	</package>
</struts>
